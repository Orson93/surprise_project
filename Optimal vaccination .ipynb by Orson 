      "source": [
        "\t# series array for the plotting that we will do\r\n",
        "\tseries_list = []\r\n",
        "\t\r\n",
        "\r\n",
        "\t# Procedure for identifying (n_min, n_vUT) which will satisfy the desired power level at the specified significance\r\n",
        "\t# criterion assuming that true VE >= 30%,40%,50%,60%,70%,80%,90% and under the null hypothesis that True VE <=ve_0\r\n",
        "\tfor true_ve in [x / 100.0 for x in range(30, 100, 10)]:\r\n",
        "\t    print('****************** TESTING FOR ASSUMED TRUE VE=' + str(true_ve) + ' ********************')\r\n",
        "\t    data_points = []\r\n",
        "\t    theta = get_theta(true_ve)\r\n",
        "\t    theta_0 = get_theta(ve_0)\r\n",
        "\t    for n_t in range(0, n_min + 1):\r\n",
        "\t        for n_v in range(0, n_t + 1):\r\n",
        "\t            pow = get_cdf(n_v, n_t, theta)\r\n",
        "\t            if pow >= desired_power:\r\n",
        "\t                p_value = get_cdf(n_v, n_t, theta_0)\r\n",
        "\t                if p_value <= 0.05:\r\n",
        "\t                    print('n_v=' + str(n_v) + ' n_t=' + str(n_t) + ' power=' + str(pow) + ' p_value=' + str(p_value))\r\n",
        "\t                    data_points.append([n_v, n_t])\r\n",
        "\t                    break\r\n",
        "\t    if len(data_points) > 0:\r\n",
        "\t        tuples = np.array(data_points)\r\n",
        "\t        series, = plt.plot(tuples[:, 1], tuples[:, 0], label='True VE=' + str(true_ve))\r\n",
        "\t        series_list.append(series)\r\n",
        "\t\r\n",
        "\r\n",
        "\t# Mark the point (n_min, n_vUT) used by Pfizer in their COVID-19 vaccine protocol document\r\n",
        "\tplt.plot(164, 53, color='red', marker='+', markeredgewidth=3, markersize=16)\r\n",
        "\t\r\n",
        "\r\n",
        "\tplt.legend(handles=series_list)\r\n",
        "\tplt.xlabel('Total number of infections observed in observation period (N_t=N_min)')\r\n",
        "\tplt.ylabel('Max infections in vaccinated group (N_v=N_vUT)')\r\n",
        "\t\r\n",
        "\r\n",
        "\tplt.show()\r\n",
        "\t\r\n",
        "\r\n",
        "\tn_v = 6\r\n",
        "\tn_t = 32\r\n",
        "\tdata_points = []\r\n",
        "\ttrue_ve = 0.3\r\n",
        "\t# Procedure for finding out Prob(n_v <=6 | ( n_t = 32 and True VE in [0.3. 0.9]) )\r\n",
        "\twhile true_ve <= 0.9:\r\n",
        "\t    theta = get_theta(true_ve)\r\n",
        "\t    prob = get_cdf(n_v, n_t, theta)\r\n",
        "\t    pt = [round(theta, 5), round(prob, 5)]\r\n",
        "\t    data_points.append(pt)\r\n",
        "\t    print('ve_true=' + str(round(true_ve, 2)) + ' ==> (theta,prob)=' + str(pt))\r\n",
        "\t    true_ve = true_ve + 0.01\r\n",
        "\t\r\n",
        "\r\n",
        "\t# Reproduce the probabilities in Table 7, Column 3 (Success in IA#2) in Pfizer's protocol spec\r\n",
        "\t# We will specifically mark these five data points on the plot along with all other points for VE in (0.3 to 0.9) range\r\n",
        "\tfor true_ve in [0.3, 0.5, 0.6, 0.7, 0.8]:\r\n",
        "\t    theta = get_theta(true_ve)\r\n",
        "\t    prob = get_cdf(n_v, n_t, theta)\r\n",
        "\t    print('E(True VE)=' + str(true_ve) + ' ==> (theta, P(n_v <=6 |( n_t = 32, theta) ))=' + str(\r\n",
        "\t        [round(theta, 5), round(prob, 3)]))\r\n",
        "\t    plt.plot(round(theta, 5), round(prob, 5), color='red', marker='o')\r\n",
        "\t    plt.text(round(theta, 5), round(prob, 5), 'VE=' + str(true_ve) + ', p=' + str(round(prob, 5)))\r\n",
        "\t\r\n",
        "\r\n",
        "\ttuples = np.array(data_points)\r\n",
        "\tseries, = plt.plot(tuples[:, 0], tuples[:, 1], label='P(n_v <=6 |( n_t = 32, theta) )')\r\n",
        "\tplt.legend(handles=[series])\r\n",
        "\tplt.xlabel('theta = (1-VE)/(2-VE)')\r\n",
        "\tplt.ylabel('P(n_v <=6 | (n_t = 32, theta) )')\r\n",
        "\tplt.show()\r\n"
      ],
      "execution_count": 9,
