      "source": [
        "import scipy.stats as stats\r\n",
        "from matplotlib import pyplot as plt\r\n",
        "import numpy as np\r\n",
        "import math\r\n",
        "\t\r\n",
        "\r\n",
        "desired_power = 0.9\r\n",
        "# Vaccine Efficacy under the Null Hypothesis\r\n",
        "ve_0 = 0.3\r\n",
        "# Significance criterion for rejecting H0\r\n",
        "alpha = 0.05\r\n",
        "\t\r\n",
        "\r\n",
        "# Minimum number of infections needed to be observed in the observation period\r\n",
        "n_min = 200\r\n",
        "# Upper bound on infections in the vaccine group. n_vUT <= n_min\r\n",
        "n_vUT = 0\r\n",
        "\t\r\n",
        "\r\n",
        "\t\r\n",
        "\r\n",
        "# the random variable of the Beta Binomial distribution. theta has the range [0,1]\r\n",
        "def get_theta(ve):\r\n",
        "\t  return (1 - ve) / (2 - ve)\r\n",
        "\t\r\n",
        "\r\n",
        "\t\r\n",
        "\r\n",
        "# CDF of the Binomial distribution\r\n",
        "def get_cdf(k, n, p):\r\n",
        "\t  return stats.binom.cdf(k, n, p)\r\n",
        "\t\r\n",
        "\r\n",
        "\t\r\n",
        "\r\n",
        "# get the Binomial probability\r\n",
        "def get_binomial_probability(k, n, p):\r\n",
        "\t  return math.pow(p, k) * math.pow(1 - p, n - k) * nCr(n, k)\r\n",
        "\t\r\n",
        "\r\n",
        "\t\r\n",
        "\r\n",
        "# n choose k\r\n",
        "def nCr(n, r):\r\n",
        "\t  f = math.factorial\r\n",
        "\t  return f(n) / (f(r) * f(n - r))\r\n"
      ],
      "execution_count": 8,
      "outputs": []
    },
